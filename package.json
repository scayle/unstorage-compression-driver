{
  "name": "@scayle/unstorage-compression-driver",
  "version": "1.2.3",
  "description": "Compression driver for unstorage",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "url": "git+https://github.com/scayle/unstorage-compression-driver.git"
  },
  "keywords": [
    "unstorage",
    "compression"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "CHANGELOG.md",
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "verify-packaging": "attw --pack . --profile esm-only",
    "format": "dprint check",
    "format:fix": "dprint fmt",
    "lint": "eslint .",
    "lint:ci": "eslint . --format gitlab",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:ci": "vitest --run --coverage --reporter=default --reporter=junit --outputFile=./coverage/junit.xml",
    "test:watch": "vitest watch",
    "test:bench:compression": "NODE_OPTIONS='--expose-gc' jiti test/benchmark/compression.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "peerDependencies": {
    "unstorage": "^1.10.1"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "unstorage": "1.17.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@scayle/eslint-config-storefront": "workspace:*",
    "@scayle/prettier-config-storefront": "workspace:*",
    "@scayle/storefront-api": "workspace:*",
    "@scayle/vitest-config-storefront": "workspace:*",
    "@vitest/coverage-v8": "3.2.4",
    "dprint": "0.50.2",
    "eslint": "9.39.1",
    "eslint-formatter-gitlab": "6.0.1",
    "jiti": "2.6.1",
    "tinybench": "5.1.0",
    "typescript": "5.9.3",
    "unbuild": "3.6.1",
    "unstorage": "1.17.2",
    "vitest": "catalog:"
  },
  "volta": {
    "node": "22.21.1"
  }
}
