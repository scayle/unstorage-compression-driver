{
  "name": "@scayle/unstorage-compression-driver",
  "version": "0.1.3",
  "description": "Compression driver for unstorage",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/rpc.js"
    },
    "./compression": {
      "types": "./dist/compression.d.ts",
      "import": "./dist/compression.js"
    }
  },
  "main": "./dist/index.js",
  "files": [
    "CHANGELOG.md",
    "./dist/*.js",
    "./dist/*.d.ts"
  ],
  "scripts": {
    "build": "yarn tsc -p tsconfig.build.json",
    "format": "dprint check",
    "format:fix": "dprint fmt",
    "lint": "eslint . --format gitlab",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:ci": "vitest run",
    "test:watch": "vitest watch"
  },
  "peerDependencies": {
    "unstorage": "^1.10.1"
  },
  "dependencies": {
    "destr": "2.0.3",
    "unstorage": "1.10.2"
  },
  "devDependencies": {
    "@scayle/eslint-config-storefront": "4.2.0",
    "@scayle/prettier-config-storefront": "2.0.2",
    "dprint": "0.46.1",
    "eslint": "9.4.0",
    "eslint-formatter-gitlab": "5.1.0",
    "@faker-js/faker": "8.4.1",
    "unstorage": "1.10.2",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vitest": "1.6.0"
  },
  "volta": {
    "node": "20.14.0"
  }
}
