{
  "name": "@scayle/unstorage-compression-driver",
  "version": "0.2.3",
  "description": "Compression driver for unstorage",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "CHANGELOG.md",
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "verify-packaging": "attw --pack .",
    "format": "dprint check",
    "format:fix": "dprint fmt",
    "lint": "eslint . --format gitlab",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:ci": "vitest --run --passWithNoTests --coverage --reporter=default --reporter=junit --outputFile=./coverage/junit.xml",
    "test:watch": "vitest watch"
  },
  "peerDependencies": {
    "unstorage": "^1.10.1"
  },
  "dependencies": {
    "destr": "2.0.3",
    "unstorage": "1.15.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.3",
    "@scayle/eslint-config-storefront": "4.4.1",
    "@scayle/prettier-config-storefront": "2.0.2",
    "dprint": "0.49.0",
    "eslint": "9.21.0",
    "eslint-formatter-gitlab": "5.1.0",
    "unbuild": "3.3.1",
    "unstorage": "1.15.0",
    "typescript": "5.7.3",
    "vite": "5.4.14",
    "vitest": "2.1.9"
  },
  "volta": {
    "node": "22.14.0"
  }
}
