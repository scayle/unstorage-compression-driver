{
  "name": "@scayle/unstorage-compression-driver",
  "version": "0.1.5",
  "description": "Compression driver for unstorage",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/rpc.js"
    },
    "./compression": {
      "types": "./dist/compression.d.ts",
      "import": "./dist/compression.js"
    }
  },
  "main": "./dist/index.js",
  "files": [
    "CHANGELOG.md",
    "./dist/*.js",
    "./dist/*.d.ts"
  ],
  "scripts": {
    "build": "yarn tsc -p tsconfig.build.json",
    "format": "dprint check",
    "format:fix": "dprint fmt",
    "lint": "eslint . --format gitlab",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:ci": "vitest --run --passWithNoTests --coverage --reporter=default --reporter=junit --outputFile=./junit.xml",
    "test:watch": "vitest watch"
  },
  "peerDependencies": {
    "unstorage": "^1.10.1"
  },
  "dependencies": {
    "destr": "2.0.3",
    "unstorage": "1.13.1"
  },
  "devDependencies": {
    "@scayle/eslint-config-storefront": "4.3.2",
    "@scayle/prettier-config-storefront": "2.0.2",
    "dprint": "0.47.6",
    "eslint": "9.16.0",
    "eslint-formatter-gitlab": "5.1.0",
    "@faker-js/faker": "9.0.3",
    "unstorage": "1.13.1",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vitest": "2.1.7"
  },
  "volta": {
    "node": "22.11.0"
  }
}
